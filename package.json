{
  "type": "module",
  "name": "kickoff-sveltekit",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/migrate",
    "db:reset": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/reset",
    "db:drop": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/drop",
    "db:studio": "drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:reset && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.17",
    "bits-ui": "^0.18.4",
    "clsx": "^2.1.0",
    "cssnano": "^6.0.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "formsnap": "^0.5.1",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.5",
    "sveltekit-superforms": "^2.6.2",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-node": "^1.3.1",
    "vitest": "^1.3.1",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.523.0",
    "@aws-sdk/s3-request-presigner": "^3.523.0",
    "@lucia-auth/adapter-drizzle": "^1.0.2",
    "@lucia-auth/adapter-postgresql": "^3.1.0",
    "@sendgrid/mail": "^8.1.1",
    "drizzle-orm": "^0.29.4",
    "lucia": "^3.0.1",
    "nanoid": "^5.0.6",
    "oslo": "^1.1.3",
    "pg": "^8.11.3",
    "svelte-email": "^0.0.4",
    "svelte-sonner": "^0.3.19",
    "sveltekit-flash-message": "^2.4.2"
  }
}
